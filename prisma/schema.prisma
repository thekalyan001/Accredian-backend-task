datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model accredian {
  id           Int      @id @default(autoincrement())
  referrerName String
  referrerEmail String
  refereeName   String
  refereeEmail  String  @unique
  referralMsg   String
  createdAt     DateTime @default(now())
}
